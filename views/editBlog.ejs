        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">
            <div class="page-header">
              <h3 class="page-title"> Form elements </h3>
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item"><a href="#">Forms</a></li>
                  <li class="breadcrumb-item active" aria-current="page">Form elements</li>
                </ol>
              </nav>
            </div>
            <div class="row">

              <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">Edit Blog Form</h4>
                    <form class="forms-sample" method="post" action="/blog/updateBlog/<%=editblog.id%>" enctype="multipart/form-data">
                      <div class="form-group">
                        <label for="exampleInputUsername1">Blog Title</label>
                        <input type="text" class="form-control" id="exampleInputUsername1" placeholder="Enter Blog Title" name="title" value="<%=editblog.title%>">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputEmail1">Description</label>
                        <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Enter Description..." name="desc" value="<%=editblog.desc%>">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputPassword1">Category</label>
                        <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter Category" name="category" value="<%=editblog.category%>">
                      </div>
                      <div class="form-group">
                        <label for="exampleInputConfirmPassword1">Blog Date</label>
                        <input type="text" class="form-control" id="exampleInputConfirmPassword1" placeholder="Enter Blog Date" name="date" value="<%=editblog.date%>">
                      </div>

                      <div class="form-group">
                        <label for="exampleInputConfirmPassword1">Blog Image</label>
                        <input type="file" class="form-control" id="exampleInputConfirmPassword1" name="blogImage" >
                        <br>
                        <img src="<%=editblog.blogImage%>" alt="" height="150" width="100" style="object-fit: cover;" >
                      </div>
                      <br>
                     
                      <button type="submit" class="btn btn-primary mr-2">Edit</button>
                    </form>
                  </div>
                </div>
              </div>

             
              
             
             
             
          
          
            
             
             
            </div>
